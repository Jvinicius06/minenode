{
  "extends": "marine/prettier/node",
  "plugins": ["import"],
  "parserOptions": {
    "project": "tsconfig.eslint.json"
  },
  "rules": {
    "import/order": [
      "error",
      {
        "groups": ["builtin", "external", "internal", "sibling", "parent"],
        "alphabetize": {
          "order": "asc"
        }
      }
    ],
    "no-console": "error"
  }
}
